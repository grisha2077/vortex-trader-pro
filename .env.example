# Binance API Keys
BINANCE_API_KEY=your_api_key_here
BINANCE_API_SECRET=your_api_secret_here

# Optional: Use testnet (true/false)
USE_TESTNET=true

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
